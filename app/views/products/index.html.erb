<div class="container">
<div class="row">
<% @products.each do |product| %>
<%= link_to product_path(product) do %>
<div class="card">
<div class="col-4" >
  <h3><%= product.name %></h3>
  <%= cl_image_tag(product.photo.key)%>
  <p><%= product.price_per_day %> euros per day</p>

</div>

</div>
<% end %>
<% end %>
</div>
</div>
</div>
