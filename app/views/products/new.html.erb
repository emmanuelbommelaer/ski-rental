<div class="container">
  <h1>Add a new product to rent</h1>
  <%= simple_form_for @product do |f| %>
    <%= f.input :category %>
    <%= f.input :name %>
    <%= f.input :details %>
    <%= f.input :price_per_day %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit, class:"btn btn-primary" %>
  <% end %>
</div>
