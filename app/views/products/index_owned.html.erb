<div class="container">
  <div class="d-flex justify-content-between">
    <h2>All my products</h2>
    <%= button_to "List a product", new_product_path, class: "btn btn-primary", method: :get %>
  </div>
  <% @products.each do |product| %>
    <p><%= product.name %></p>
    <%= cl_image_tag(product.photo.key) %>
    <%= link_to "See details", product_path(product) %>
  <% end %>
</div>
